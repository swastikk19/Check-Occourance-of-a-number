#include <bits/stdc++.h>
using namespace std;

int main()
{
    //write your code here
    int t, n;
    cin >> t;
    while (t--)
    {
        int sum = 0;
        cin >> n;
        while (n != 0)
        {
            if (n % 10 == 5)
            {
                sum++;
            }
            n = n / 10;
        }
        cout << sum << endl;
    }
    return 0;
}
